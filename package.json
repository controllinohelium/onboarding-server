{
  "name": "onboarding-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "DEBUG=* nodemon --exec babel-node ./src/server.js",
    "build": "babel src --out-dir build --copy-files",
    "start": "node build/server.js",
    "clean": "rimraf build",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo"
  },
  "engines": {
    "node": "14.13.x"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@helium/crypto": "^3.7.0",
    "@helium/transactions": "^3.11.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  }
}
